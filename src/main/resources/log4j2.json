{
  "configuration": {
    "status": "error",
    "ThresholdFilter": {
      "level": "debug"
    },
    "appenders": {
      "Console": {
        "name": "STDOUT",
        "PatternLayout": {
          "pattern": "%d{HH:mm:ss} [%t] %-5level %logger{36} - %msg%n"
        }
      }
    },
    "loggers": {
      "root": {
        "level": "debug",
        "AppenderRef": {
          "ref": "STDOUT"
        }
      }
    }
  }
}